$@ int64 fact int a
    ? a
        => * a @fact - a 1
    !
        => 1

$@ void factor int num int curD
    ? < // num curD 1
        << curD
        @factor / num curD curD
    !? > * curD curD num
        ? > num 1
            << num
    !
        @factor num + 1 curD

$@ int main
    $ int a
    >> a
    ;<< @ fact a
    @factor a 2
